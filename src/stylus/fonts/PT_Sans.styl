@font-face {
	font-family: 'PT_SansRegular';
	src: url('fonts/PT_Sans-Regular/PT_Sans-Web-Regular.eot');
	src: local('☺'), url('fonts/PT_Sans-Regular/PT_Sans-Web-Regular.woff') format('woff'), url('fonts/PT_Sans-Regular/PT_Sans-Web-Regular.ttf') format('truetype'), url('fonts/PT_Sans-Regular/PT_Sans-Web-Regular.svg') format('svg');
	font-weight: normal;
	font-style: normal;
}

@font-face {
	font-family: 'PT_SansBold';
	src: url('fonts/PT_Sans-Bold/PT_Sans-Web-Bold.eot');
	src: local('☺'), url('fonts/PT_Sans-Bold/PT_Sans-Web-Bold.woff') format('woff'), url('fonts/PT_Sans-Bold/PT_Sans-Web-Bold.ttf') format('truetype'), url('fonts/PT_Sans-Bold/PT_Sans-Web-Bold.svg') format('svg');
	font-weight: normal;
	font-style: normal;
}